# Monad Testnet Configuration
RUNNER_CHAIN_ID=10143
RUNNER_RPC_HTTP_URL=https://testnet-rpc.monad.xyz

# Contract Addresses (set after deployment)
RUNNER_BOT_REGISTRY=0x0000000000000000000000000000000000000000

# Indexing
RUNNER_START_BLOCK=0
RUNNER_OUT_DIR=out
RUNNER_MAX_BOTS=
RUNNER_PORT=8787
RUNNER_DB_PATH=out/runner.db
RUNNER_DISABLE_LOOP=false
RUNNER_TICK_INTERVAL_SECONDS=30
RUNNER_TRADE_PROB=0.35
RUNNER_TRADE_MIN_COOLDOWN_SECONDS=300
# Optional: protects POST /admin/tick
# RUNNER_ADMIN_TOKEN=change-me
# Optional: protects POST /bots/:botId/proposals and POST /bots/:botId/connector/heartbeat (local connector writes)
# If set, requires X-Runner-Connector-Token header for proposals and heartbeat POST.
# If unset, heartbeat POST is allowed from localhost without token (proposals POST still requires it).
# RUNNER_CONNECTOR_TOKEN=change-me

# PnL Configuration (optional)
RUNNER_QUOTE_MODE=none
# RUNNER_QUOTE_MODE=uniswapV2
# RUNNER_QUOTE_ROUTER=0x...
# RUNNER_QUOTE_BASE_TOKEN=USDC

# Execution (DRY-RUN ONLY - no private keys needed for now)
# When EIP-712 TradeIntent spec is available, add:
# RUNNER_OPERATOR_PRIVATE_KEY=0x...
